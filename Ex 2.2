x = 5
x == 5 and 3                  # 3    --> zwraca 3 (ostatni int), bo wynikiem jest prawda i cokolwiek, 5 jest prawdą więc dostajemy cokolwiek
x == 4 and 3                  # False --> zwraca False, bo x nie jest równe 4 (pierwsza wartość nie jest prawdą)
3 and x == 5                  # True  --> zwraca True, bo x jest równy 5 a 3 nie bierze udziału w równaniu
3 and x == 4                  # False --> zwraca False, bo x nie jest równy 4

isinstance(True, int)         # True --> true ma też wartość 1, więc należy do int
isinstance(True, bool)        # True --> True należy do bool
